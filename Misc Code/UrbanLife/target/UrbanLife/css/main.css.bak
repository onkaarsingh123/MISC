body{
	font-family: 'Roboto', sans-serif;	
	color:#fff;
}
md-content.md-default-theme, .main-content{
	background:#34495e;
}
md-select-menu md-content.md-default-theme{
	background:#ecf0f1;
	padding-bottom:auto;
}
md-toolbar.md-default-theme.header{
	height:50px;
	background:#dbdbdb;
}
md-toolbar.header.dark-header{
	height:80px;
	background:#2b2d2c;
	border-bottom:solid 1px #fff;
}

.header h5{
	margin-right:15px;
	font-weight:300;
}
.header h5 a{
	color:#16a085;
}
h1.logo{
	font-weight: 500;
	color:#fff;
	margin-top:15px;
	font-size:15px;
	margin:0;
	padding:23px 0 23px 10px;
	background:#16a085;
	width:90px;
}
.dark-header h1.logo{
	font-weight: 100;
	color:#fff;
	margin-top:15px;
	font-size:36px;
	background:none;
	width:auto;
	margin-top: 0;
}

h4,h5{
	margin-bottom:0;
}
.black-btn.md-button.md-default-theme.md-primary.md-raised, 
.black-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):hover, 
.black-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):focus{
	background-color:#5d6d7e !important;;
	color:#c9c9c9;
	text-transform:none;
	padding:8px 20px;
}
.blue-btn.md-button.md-default-theme.md-primary.md-raised, 
.blue-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):hover, 
.blue-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):focus{
	background-color:#16a085 !important;
	color:#fff;
	text-transform:none;
	padding:8px 20px;
}
.orange-btn.md-button.md-default-theme.md-primary.md-raised, 
.orange-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):hover, 
.orange-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):focus{
	background-color: #d03a1d!important;
	color:#fff;
	text-transform:none;
	padding:8px 20px;
}
.hollow-btn.md-button.md-default-theme.md-primary.md-raised, 
.hollow-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):hover, 
.hollow-btn.md-button.md-default-theme.md-primary.md-raised:not([disabled]):focus{
	border:solid 1px #666;
	background-color:transparent !important;
	color:#5e5c5c;
	text-transform:none;
	padding:8px 20px;
}
.reg-btns .md-button{
	margin-right:15px;
}
.form-content{
	padding:15px;
	height:100%;
	margin:0 0 20px 20px;
}
.form-content,.extra-content{
	background:#ecf0f1;
}
.reg-top .hz-line{
	height: 10px;
	width: 140px;
	background: #29394a;
	display: inline-block;
	margin: 0 -7px;
	border-top: 1px solid #212e3c;
}
.reg-top .md-button.md-default-theme.md-primary.md-fab{	
	background-color:#e3998b!important;
}
.reg-top .md-button.md-default-theme.md-primary.md-raised,
.md-button.md-default-theme.md-primary.md-raised:not([disabled]):hover, .md-button.md-default-theme.md-primary.md-raised:not([disabled]):focus, .md-button.md-default-theme.md-primary.md-fab:not([disabled]):hover, .md-button.md-default-theme.md-primary.md-fab:not([disabled]):focus{
	background-color:#d03a1d!important;
	border:solid 2px #273646;
}
.md-button.md-default-theme.md-primary.md-raised, 
.md-button.md-default-theme.md-primary.md-fab{
	background-color:#d03a1d!important;
}
.reg-top-title{
	margin:0 30px 0 20px;
	color:#fff;
	font-weight:300;
}
md-toolbar.md-default-theme.footer{
	background-color:#2c3e50;
	height:50px;
}
.footer h6{
	margin:5px 15px 0 0;
}
.validation-messages{
	/*position:absolute;*/
	bottom: 6px;
	font-size: 12px;
	color: red;
}
.upload-photo{
	margin-top: 34px;
}
.upload-photo h5{
	margin-top:0;
}
.upload-photo h5 a{
	color:#36a4d9;
}

.file-upd-icon{
	background:url("../images/sprites.png") no-repeat 0 0;
	padding:26px;
	display:inline-block;
	margin-left:12px;
}
.mandate-field{
	color:#db7575;
}
.ccnamemsg{
	position:absolute;
	right:0;
	top:20px;
	font-weight:300;
}
.pen-icon, 
.visa-icon, 
.mastercard-icon, 
.amex-icon, 
.cardsback-icon, 
.tripadvisior-icon, 
.dashboard .icon,
.create-deals .icon,
.view-deals .icon,
.billings .icon, 
.bell-icon, 
.setting-icon, 
.home-circles .circle,
.availableon > div > div{
	background: url("../images/sprites.png") no-repeat -68px 0;
	padding: 0 10px;
	margin-left: 5px;
}
.cctypes{
	border-bottom:solid 1px #b2aaaa;
	padding-bottom:8px;
}
.cctypes .md-switch-thumb, md-radio-button{
	margin:0;
}
.cctypes .md-switch-thumb .md-label, md-radio-button .md-label{
	margin-right:0;
	margin-left:-3px;
}
.cctypes md-radio-button{
	display:inline;
}
.visa-icon{
	background-position: -108px 0;
	padding: 5px 26px;
}
.mastercard-icon{
	background-position: -179px 0;
	padding: 5px 26px;
}
.amex-icon{
	background-position: -248px 0;
	padding: 5px 26px;
}
.cardsback-icon {
	background-position: -320px 0;
	padding: 17px 26px;
	position: absolute;
	left: 110px;
	top: 11px;
}
.select-box h5{
	margin-top:0;
}
.select-box md-select{
	margin-top:12px;
}
.match-label{
	font-size:12px;
	font-weight:300;
	color:#6c6e6f;
}
/*business pages*/
md-sidenav, md-sidenav.md-locked-open, md-sidenav.md-closed.md-locked-open-add-active {
	min-width: 90px !important;
	max-width: 90px !important;
}
md-sidenav{
	background:#203141;
	color:#fff;
}
.trip-advisior{
	margin-top:15px;
	border-bottom: solid 1px #b2aaaa;
}
.tripadvisior-icon{
	background-position: -2px -74px;
	padding: 36px;
	margin-right:20px;
}
.custom-input{
	background:none;
	border:none;
	border-bottom: solid 1px #b2aaaa;
	
}
.trip-advisior .custom-input{
	
}
.self-button{
	margin-right:20px;	
}
.reg-top h2{
	display:inline-block;
}
.dashboard .icon,
.create-deals .icon,
.view-deals .icon,
.billings .icon{
	display:block;
	display: inline-block;
	padding: 21px;
	background-position: -90px -39px;
	margin:8px 0 0 23px;
}
.create-deals .icon{
	background-position: -85px -80px;
}
.view-deals .icon{
	background-position: -143px -40px;
}
.billings .icon{
	background-position: -138px -79px;
}
md-sidenav h5{
	margin-top:0;
	text-align:center;
}

md-sidenav > div:first-child{
	border-top:solid 1px #151515;
}
md-sidenav > div{
	height:87px;
	border-bottom:solid 1px #151515;
	border-right:solid 1px #151515;
}
.top-right ul{
	list-style:none;
	display:inline-block;
	margin-top:12px;
}
.top-right ul li:first-child{
	border-left:none;
}
.top-right ul li:last-child{
	border-right:none;
}

.top-right ul li{
	float:left;
	border-right:solid 1px #fff;
	border-left:solid 1px #cfcfcf;
	color:#000;
	padding:0 10px;
	height:32px;
}
.top-right .location-sel{
	margin-top:5px;
}
.profile-photo{
	height:37px;
	width:37px;
	border:solid 1px #b5b5b5;
	display:inline-block;
	border-radius:100px;
	margin-right:10px;
}
.bell-icon{
	background-position:-199px -45px;
} 
.setting-icon{
	background-position:-196px -83px;
}
.welcome-user{
	vertical-align:top;
	margin-top:14px;
}
.occupancies{
	margin: 20px 0;
}
.operating-hrs-grid{
	border-bottom:1px solid #b2aaaa;
	margin-bottom:20px;
}
.operating-hrs md-select{
	margin-right:10px;
}
.heading-row{
	background:#c7c7c7;
}
.heading-row > div{
	border-left:1px solid #fff;
	padding:7px;
}
.heading-row > div:first-child{
	border-left:1px solid #fff;
}
/*login*/
.login-top{
	color:#fff;
	text-align:center;
}
.login-top h1{
	font-size:42px;
	font-weight:normal;
}
.login-top h2{
	font-size:18px;
	font-weight:normal;
}
md-whiteframe{
	background:#fff;
	border-radius:6px;
	width:500px;
	margin-top:50px;
}
md-whiteframe md-input-container, md-whiteframe .login-controls{
	width:80%;
}
md-whiteframe md-checkbox{
	margin-left:0;
}
.login-head{
	background:#2b2d2c;
	height:30px;
}
.login-page .main-content{
	background:url("../images/img_bg.png") no-repeat center center; 
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
}
.login-page md-content.md-default-theme, 
.welcomePage md-content.md-default-theme,
.homePage md-content.md-default-theme{
	background:none;
}
body.login-page{
	background:#000;
}
.login-controls button{
	margin:10px 0;
}
.top-headings{
	color:#fff;
}
.abcdPage .main-content md-content{
	padding: 25px 30px;
}
.abcdPage .main-content .top-headings{
	border-bottom:solid 2px #5d7388;
}
.abcdPage .main-content md-content > div{
	padding: 20px 0;
}
.abcdPage .main-content .create-deals, .abcdPage .main-content .add-business{
	text-align: center;
	color:#888f97;
}
.abcdPage .add-business{
	border-right: solid 2px #5d7388;
	color:#fff;
}
.abcdPage .create-deals h4, .abcdPage .add-business h4{
	margin-top:0;
}
.abcdPage .create-deals .md-button, .abcdPage .add-business .md-button{
	margin-top:20px;
}
.abcdPage .add-business .md-button{
	margin-top:37px;
}
/*Welcome page*/
.welcomePage .top-headings{
	margin-left:30px;
}
.welcomePage .main-content{
	background:url("../images/bg-welcome.png") no-repeat 0 0 #000;
}
.black-overlay{
	background-color:rgba(0, 0, 0, 0.4);
	margin:0 30px;
	margin-top:30px;
	padding:50px 0 15px;
}
.offer-circles{
	margin-bottom:30px;
}
.offer-circles .hr-bar {
	background:#d03a1d;
	height:4px;
}
.offer-circles > div:last-child{
	padding-top:4px;
}
.offer-circles .md-button.md-default-theme.md-primary.md-raised, 
.offer-circles .md-button.md-default-theme.md-primary.md-fab{
	box-shadow: 0 0 0 10px rgba(27, 185, 231, 0.2);
}
.welcome-text h4{
	margin-top:5px;
}
.welcome-controls{
	margin-top:20px;
}
.welcome-controls button:first-child{
	margin-right:20px;
}
.orange-text{
	color:#d03a1d;
}
.faq-link a{
	margin-right:50px;
	text-decoration:none;
}

/*Home page */
.top-right button{
	margin:20px 20px 0 0;
	font-size:18px;
	
}
.bghome{
	background:url("../images/bg-home.png") no-repeat 0 0 #1d1d1e;
	-moz-background-size:100% auto;
	-webkit-background-size:100% auto;
	background-size:100% auto;
	padding-bottom:2px;
}
.font-normal{
	font-weight:normal;
}
.font-thin{
	font-weight:100;
}
.font-light{
	font-weight:300;
}
.font-bold{
	font-weight:bold;
}
.homePage .top-headings h4{
	margin-top:0
}
.home-video{
	margin:40px 0;
}
.home-signup a, .reached-places h1{
	color:#16a085;
}
.network-message h1{
	font-size:78px;
	margin-bottom:0;
}
.home-circles{
	margin-top:50px;
}
.home-circles .circle{
	border-radius:100px;
	border:solid 17px #cfd8da;
	height:189px;
	width:189px;
}
.home-circles .md-text h1{
	font-size:87px;
	margin-bottom:0;
	margin-top:40px;
}
.home-circles .md-text h1 sup{
	font-size:40px;
}
.home-circles .md-text h2{
	font-size:30px;
	margin:0;
}
.home-circles .merchant-counting{
	background-position:15px -143px;
} 
.home-circles .deals-listed{
	background-position:-116px -135px;
} 
.home-circles .happy-customer{
	background-position:-257px -137px;
}
.reached-places{
	margin-top:50px;
}
.reached-places h1{
	font-size:48px;
}
.reached-places li{
	font-size:24px;
	margin:15px 0;
	font-weight:300;
}
.subfooter{
	background:#232324;
	padding-top:20px;
}
.subfooter > div{
	border-right:solid 1px #111112;
	border-left:solid 1px #39393b;
	padding-bottom:15px;
}
.subfooter > div:first-child{
	border-left:none;
}
.subfooter > div:last-child{
	border-right:none;
}

.subfooter h2{
	font-size:34px;
	font-weight:400;
}
.subfooter p{
	margin:0;
	line-height:20px;
}
.availableon{
	padding:20px 0;
	background:#232324;
}
.availableon h2{
	font-size:32px;
}
.availableon > div > div{
	height: 56px;
	width: 179px;
}
.availableon > div > div.appstore{
	background-position:-2px -302px;
	margin-right:30px;
}
.availableon > div > div.googleplay{
	background-position:-219px -303px;
}
.homefooter{
	background:#232324;
	border-top:solid 1px #353538;
}
.homefooter h5{
	margin-left:30px;
}
.homefooter li{
	list-style:none;
}
.homefooter li a{
	color:#fff;
	text-decoration:none;
	font-size:12px;
}
.home-copyright{
	margin:20px 20px 0 0;
}